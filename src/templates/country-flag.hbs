<li>
    <h2>{{name}}</h2>
    <div>
        <img src="{{flag}}" alt="Флаг" width="120">
        <div>
            <p>Capital: {{capital}}</p>
            <p>Population: {{population}}</p>

            <ul>
                {{#each languages}}
                <li>{{this.name}}</li>
                {{/each}}
            </ul>
        </div>
    </div>
</li>